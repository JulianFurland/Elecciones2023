@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

</head>
<body>
    @foreach (Partido par in ViewBag.ListaPartidos)
    {
         <div class="divTarjeta">
        <img src="/imgs/@par.Logo" alt="img" width="300px">
        <h3>@par.Nombre</h3>
        <a href='@Url.Action("VerDetallePartido", new {IdPartido = par.IdPartido})' class="boton">Ver Partido</a>
    </div>
    }
</body>
</html>